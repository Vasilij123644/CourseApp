<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.projectApp.projectCourseApp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GoshaDudarAndroidFirst">
        <activity
            android:name="com.projectApp.projectCourseApp.Contacts"
            android:exported="false" />
        <activity
            android:name="com.projectApp.projectCourseApp.credits"
            android:exported="false" />
        <activity android:name="com.projectApp.projectCourseApp.OrderPage" />
        <activity android:name="com.projectApp.projectCourseApp.CoursePage" />
        <activity android:name="com.projectApp.projectCourseApp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>